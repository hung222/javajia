{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "python manage.py runserver"
  },
  "deploy": {
    "startCommand": "python manage.py runserver",
    "healthcheckPath": "/heath",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
